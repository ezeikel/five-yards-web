{
  "name": "five-yards-web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "jest --config test/jest.lint.js",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json|ts|tsx|css|html|md)\"",
    "format": "yarn prettier -- --write",
    "check-format": "yarn prettier -- --list-different",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "pretest:e2e:run": "yarn build",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "validate": "yarn test:coverage && yarn test:e2e:run",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test:e2e:run"
    }
  },
  "lint-staged": {
    "**/*.+(js|jsx|json|ts|tsx|css|html|md)": [
      "prettier --write",
      "jest --findRelatedTests",
      "git add"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.2.7",
    "@fortawesome/fontawesome-svg-core": "^1.2.32",
    "@fortawesome/free-brands-svg-icons": "^5.15.1",
    "@fortawesome/free-solid-svg-icons": "^5.15.1",
    "@fortawesome/pro-duotone-svg-icons": "^5.15.1",
    "@fortawesome/pro-light-svg-icons": "^5.15.1",
    "@fortawesome/pro-regular-svg-icons": "^5.15.1",
    "@fortawesome/pro-solid-svg-icons": "^5.15.1",
    "@fortawesome/react-fontawesome": "^0.1.12",
    "@sentry/browser": "^5.27.4",
    "@sentry/node": "^5.27.4",
    "@sentry/webpack-plugin": "^1.13.0",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.11.0",
    "@zeit/next-css": "^1.0.1",
    "@zeit/next-source-maps": "0.0.4-canary.1",
    "apollo-cache-persist": "^0.1.1",
    "apollo-link-error": "^1.1.13",
    "apollo-link-http": "^1.5.17",
    "apollo-upload-client": "^12.1.0",
    "body-scroll-lock": "^3.1.5",
    "date-fns": "^2.16.1",
    "dotenv": "^8.2.0",
    "formik": "^2.2.5",
    "graphql": "^14.0.2",
    "graphql-tag": "^2.11.0",
    "next": "^10.0.2",
    "prop-types": "^15.7.2",
    "query-string": "^6.13.7",
    "react": "^17.0.1",
    "react-adopt": "^0.6.0",
    "react-device-detect": "^1.14.0",
    "react-dom": "^17.0.1",
    "react-dropzone": "^10.1.9",
    "react-helmet": "^5.2.0",
    "react-modal": "^3.10.1",
    "react-toastify": "^5.4.0",
    "react-transition-group": "^4.4.1",
    "styled-components": "^5.2.1",
    "yup": "^0.28.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.1",
    "@testing-library/cypress": "^5.3.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^9.4.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.1",
    "babel-plugin-styled-components": "^1.11.1",
    "babel-preset-react-app": "^9.1.2",
    "casual": "^1.6.2",
    "cypress": "^4.0.2",
    "eslint": "^7.13.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "faker": "^4.1.0",
    "graphql-tools": "^4.0.5",
    "husky": "^4.3.0",
    "is-ci-cli": "^2.1.2",
    "jest": "^26.6.3",
    "jest-runner-eslint": "^0.7.6",
    "jest-styled-components": "^7.0.3",
    "jest-transform-graphql": "^2.1.0",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "lint-staged": "^10.5.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.0",
    "react-is": "^16.13.1",
    "start-server-and-test": "^1.11.6"
  }
}
