{
  "name": "five-yards-web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "test": "react-scripts test --env=jsdom"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.7",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.4.2",
    "@fortawesome/pro-duotone-svg-icons": "^5.11.2",
    "@fortawesome/pro-light-svg-icons": "^5.11.2",
    "@fortawesome/pro-regular-svg-icons": "^5.11.2",
    "@fortawesome/pro-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "^0.1.15",
    "apollo-cache-inmemory": "^1.3.11",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.4.7",
    "apollo-link": "^1.2.3",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.5",
    "apollo-link-state": "^0.4.2",
    "apollo-upload-client": "^11.0.0",
    "babel-core": "^6.26.3",
    "babel-plugin-styled-components": "^1.10.6",
    "date-fns": "^1.29.0",
    "formik": "^1.5.8",
    "graphql": "^14.0.2",
    "next": "^9.0.7",
    "next-with-apollo": "^4.3.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.6.2",
    "query-string": "^6.2.0",
    "react": "^16.10.1",
    "react-adopt": "^0.6.0",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.10.1",
    "react-dropzone": "^10.1.9",
    "react-helmet": "^5.2.0",
    "react-modal": "^3.10.1",
    "react-router-dom": "^4.3.1",
    "react-scripts": "2.0.5",
    "react-stripe-checkout": "^2.6.3",
    "react-toastify": "^5.4.0",
    "react-transition-group": "^2.5.0",
    "styled-components": "^4.4.0",
    "sweetalert2": "^7.28.2",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "babel-plugin-module-resolver": "^3.2.0",
    "casual": "^1.6.2",
    "enzyme-to-json": "^3.4.2",
    "graphql-tools": "^4.0.5",
    "jest": "^24.9.0",
    "jest-transform-graphql": "^2.1.0",
    "react-testing-library": "^5.0.1"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/jest.setup.js",
    "testPathIgnorePatterns": [
      "<rootDir>/.next/",
      "<rootDir>/node_modules/"
    ],
    "transform": {
      "\\.(gql|graphql)$": "jest-transform-graphql",
      ".*": "babel-jest",
      "^.+\\.js?$": "babel-jest"
    }
  },
  "//": "This is our babel config, I prefer this over a .babelrc file",
  "babel": {
    "env": {
      "development": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "production": {
        "presets": [
          "next/babel"
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      },
      "test": {
        "presets": [
          [
            "next/babel",
            {
              "preset-env": {
                "modules": "commonjs"
              }
            }
          ]
        ],
        "plugins": [
          [
            "styled-components",
            {
              "ssr": true,
              "displayName": true
            }
          ]
        ]
      }
    }
  }
}
