{
  "name": "five-yards-web",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "12.x"
  },
  "scripts": {
    "dev": "next",
    "build": "next build",
    "start": "next start -p 3000",
    "lint": "jest --config test/jest.lint.js",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json|ts|tsx|css|html|md)\"",
    "format": "yarn prettier -- --write",
    "check-format": "yarn prettier -- --list-different",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "pretest:e2e:run": "yarn build",
    "test:e2e": "is-ci \"test:e2e:run\" \"test:e2e:dev\"",
    "test:e2e:run": "start-server-and-test start http://localhost:3000 cy:run",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000 cy:open",
    "validate": "yarn test:coverage && yarn test:e2e:run",
    "storybook": "start-storybook -p 9001 -c .storybook"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && yarn test:e2e:run"
    }
  },
  "lint-staged": {
    "**/*.+(js|jsx|json|ts|tsx|css|html|md)": [
      "prettier --write",
      "jest --findRelatedTests",
      "git add"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.7",
    "@fortawesome/free-brands-svg-icons": "^5.5.0",
    "@fortawesome/free-solid-svg-icons": "^5.4.2",
    "@fortawesome/pro-duotone-svg-icons": "^5.11.2",
    "@fortawesome/pro-light-svg-icons": "^5.11.2",
    "@fortawesome/pro-regular-svg-icons": "^5.11.2",
    "@fortawesome/pro-solid-svg-icons": "^5.11.2",
    "@fortawesome/react-fontawesome": "^0.1.3",
    "@zeit/next-css": "^1.0.1",
    "apollo-boost": "^0.1.15",
    "apollo-cache-inmemory": "^1.3.11",
    "apollo-cache-persist": "^0.1.1",
    "apollo-client": "^2.4.7",
    "apollo-link": "^1.2.3",
    "apollo-link-error": "^1.1.1",
    "apollo-link-http": "^1.5.5",
    "apollo-link-state": "^0.4.2",
    "apollo-upload-client": "^11.0.0",
    "babel-core": "^6.26.3",
    "babel-plugin-styled-components": "^1.10.6",
    "date-fns": "^1.29.0",
    "formik": "^2.0.3",
    "graphql": "^14.0.2",
    "next": "^9.2.2",
    "next-with-apollo": "^4.3.0",
    "nprogress": "^0.2.0",
    "prop-types": "^15.7.2",
    "query-string": "^6.2.0",
    "react": "^16.10.1",
    "react-adopt": "^0.6.0",
    "react-apollo": "^2.1.11",
    "react-dom": "^16.10.1",
    "react-dropzone": "^10.1.9",
    "react-helmet": "^5.2.0",
    "react-modal": "^3.10.1",
    "react-router-dom": "^4.3.1",
    "react-stripe-checkout": "^2.6.3",
    "react-toastify": "^5.4.0",
    "react-transition-group": "^2.5.0",
    "styled-components": "^5.0.1",
    "sweetalert2": "^7.28.2",
    "yup": "^0.26.6"
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@storybook/react": "^5.3.14",
    "@testing-library/jest-dom": "^5.1.1",
    "@testing-library/react": "^9.4.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "casual": "^1.6.2",
    "cypress": "^4.0.2",
    "enzyme-to-json": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-import-resolver-jest": "^3.0.0",
    "eslint-plugin-cypress": "^2.10.3",
    "eslint-plugin-jest": "^23.8.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.3",
    "faker": "^4.1.0",
    "graphql-tools": "^4.0.5",
    "husky": "^4.2.3",
    "is-ci-cli": "^2.0.0",
    "jest": "^25.1.0",
    "jest-runner-eslint": "^0.7.6",
    "jest-styled-components": "^7.0.0",
    "jest-transform-graphql": "^2.1.0",
    "jest-watch-select-projects": "^2.0.0",
    "jest-watch-typeahead": "^0.4.2",
    "lint-staged": "^10.0.8",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.19.1",
    "react-testing-library": "^5.0.1",
    "start-server-and-test": "^1.10.8"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
