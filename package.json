{
  "name": "five-yards-web",
  "author": "Ezeikel Pemberton <hi@ezeikel.dev>",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "start": "next start",
    "lint": "jest --config test/jest.lint.ts",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore --write \"**/*.+(js|jsx|json|ts|tsx|css|html|md)\"",
    "format": "bun prettier --write",
    "check-format": "bun prettier --list-different",
    "test": "is-ci \"test:coverage\" \"test:watch\"",
    "test:coverage": "jest --coverage",
    "test:watch": "jest --watch",
    "test:debug": "node --inspect-brk ./node_modules/jest/bin/jest.js --runInBand --watch",
    "prepare": "husky install",
    "preinstall": "npx only-allow bun"
  },
  "jest-runner-eslint": {
    "cliOptions": {
      "ignorePath": "./.gitignore"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "tsc-files --noEmit ./next-env.d.ts"
    ],
    "**/*.+(js|jsx|json|ts|tsx|html|md)": [
      "prettier --ignore-path .gitignore --write",
      "jest --findRelatedTests --passWithNoTests"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.8.10",
    "@apollo/experimental-nextjs-app-support": "^0.6.0",
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
